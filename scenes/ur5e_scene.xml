<!--        </body>-->

        <body name="scene" pos="0 0 0">
        </body>

        <body name="table_front" pos="0.8 0 0">
            <geom name="table_front" size="0.2 0.68 0.17" type="box" rgba="0.9 0.9 0.9 1"/>
        </body>

        <body name="table_left" pos="-0.25 1 0">
            <geom name="table_left" size="0.68 0.25 0.17" type="box" rgba="0.9 0.9 0.9 1"/>
