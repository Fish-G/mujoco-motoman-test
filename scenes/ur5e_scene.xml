<mujoco model="World">
    <option integrator="implicitfast" cone="elliptic" solver="CG" noslip_iterations="2">
        <flag warmstart="disable" multiccd="enable"/>
    </option>
    <asset>
        <texture type="skybox" builtin="flat" rgb1="1 1 1" rgb2="0 0 0" width="512" height="512"/>
        <texture name="grid" type="2d" builtin="checker" width="512" height="512" rgb1=".1 .2 .3" rgb2=".2 .3 .4"/>
        <material name="grid" texture="grid" texrepeat="0.5 0.5" texuniform="true" specular="0" shininess="0"
                  reflectance="0" emission="1"/>
    </asset>
    <worldbody>
        <geom name="floor" size="20 20 .05" type="plane" material="grid" condim="3"/>
        <light pos="-0.5 0.5 5" dir="0 0 -1" castshadow="false" diffuse="1 1 1"/>
